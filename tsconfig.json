{
    "compilerOptions": {
        "module": "commonjs",
        "target": "es5",
        "alwaysStrict": true,
        "noImplicitAny": true,
        "noImplicitThis": true,
        "sourceMap": false,
        "noUnusedLocals": true,
        "strictNullChecks": true,
        //"downlevelIteration": true,
        "jsx": "react",
        "lib": [ "es6", "dom" ],
        "outDir": "build"
    },
    "include": [ "src/**/*" ]
}